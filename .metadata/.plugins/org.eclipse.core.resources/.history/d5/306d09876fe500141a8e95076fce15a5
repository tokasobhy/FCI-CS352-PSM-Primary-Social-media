package com.FCI.SWE.Post;

public class PagePost extends Post {

	public String pageOwner;
	public int numOfSeen;
	
	public PagePost(String postOwner, String postContent, int numOfLikes,
			String privacy, int membersShares,String pageOwner,int numOfSeen) {
		super(postOwner, postContent, numOfLikes, privacy, membersShares);
		this.pageOwner = pageOwner;
		this.numOfSeen = numOfSeen;
	}
	
	public PagePost(String pageOwner,int numOfSeen){
		this.pageOwner = pageOwner;
		this.numOfSeen = numOfSeen;
	}
	
	public String getPageOwner() {
		return pageOwner;
	}


	public void setPageOwner(String pageOwner) {
		this.pageOwner = pageOwner;
	}


	public int getNumOfSeen() {
		return numOfSeen;
	}


	public void setNumOfSeen(int numOfSeen) {
		this.numOfSeen = numOfSeen;
	}
	
	public String getPostOwner() {
		return postOwner;
	}


	public void setPostOwner(String postOwner) {
		this.postOwner = postOwner;
	}


	public String getPostContent() {
		return postContent;
	}


	public void setPostContent(String postContent) {
		this.postContent = postContent;
	}


	public int getNumOfLikes() {
		return numOfLikes;
	}


	public void setNumOfLikes(int numOfLikes) {
		this.numOfLikes = numOfLikes;
	}


	public String getPrivacy() {
		return privacy;
	}


	public void setPrivacy(String privacy) {
		this.privacy = privacy;
	}


	public int getMembersShares() {
		return membersShares;
	}


	public void setMembersShares(int membersShares) {
		this.membersShares = membersShares;
	}
	
	


	@Override
	public void writePost() {
		// TODO Auto-generated method stub
		
	}
	
	

}
